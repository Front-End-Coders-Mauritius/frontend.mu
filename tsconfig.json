{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "strictNullChecks": true,
    "types": ["unplugin-icons/types/astro", "vite/client"],
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"],
      "@user/*": ["src/components/user/*"],
      "@store/*": ["src/store/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
